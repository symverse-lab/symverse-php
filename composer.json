{
    "name": "symverse-lab/symverse-php",
    "description": "A library integrating Symverse Tx Raw with typed PHP.",
    "minimum-stability":"dev",
    "autoload": {
        "psr-4": {
            "Symverse\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/symverse-lab/symverse-php"
        }
    ],
    "require": {
        "n-other/php-sha3": "dev-master",
        "kornrunner/secp256k1": "0.1.2",
        "web3p/rlp": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
